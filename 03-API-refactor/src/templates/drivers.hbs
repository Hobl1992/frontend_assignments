<h1>Drivers</h1>
<div id="paging"></div>
<table class="table table-bordered">
    <thead>
    <tr>
        <th>Given Name</th>
        <th>Family Name</th>
        <th>Date of birth</th>
        <th>WikiLink</th>
    </tr>
    </thead>
    <tbody>
    {{#each drivers}}
    <tr>
        <td>{{link (concat '/driver/' this.driverId) this.givenName}}</td>
        <td>{{link (concat '/driver/' this.driverId) this.familyName}}</td>
        <td>{{moment this.dateOfBirth}}</td>
        <td>{{link this.url 'Wikipedia'}}</td>
    </tr>
    {{/each}}
    </tbody>
</table>
